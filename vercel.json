{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/files/(.*)",
      "status": 404,
      "continue": false
    },
    {
      "src": "/assets/(.*)",
      "status": 404,
      "continue": false
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 